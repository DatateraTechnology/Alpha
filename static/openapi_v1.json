{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Publish Dataset API",
    "description": "An API to return Datatoken address for the published dataset"
  },
  "paths": {
    "/alpha/1.0/publishdataset": {
      "get": {
        "tags": [
          "PublishDataset"
        ],
        "description": "Returns Datatoken address for the published dataset",
        "responses": {
          "200": {
            "description": "Datatoken address for the published dataset",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Ox.."
                }
              }
            }
          }
        }
      }
    },
    "/alpha/1.0/publishdataset/{wallet_address}": {
      "get": {
        "tags": [
          "PublishDataset with Parameter"
        ],
        "description": "Returns Datatoken address for the published dataset",
        "parameters": [
          {
            "in": "path",
            "name": "wallet_address",
            "required": false,
            "description": "wallet address",
            "schema": {
              "type": "string",
              "example": "Ox..."
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Datatoken address for the published dataset",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Ox.."
                }
              }
            }
          }
        }
      }
    }
  }
}
